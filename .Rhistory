tuneSVMR<-function(x){
gamma<-x[1];cost<-x[2]
lagDD<-lagx("DD",lag=5,insample = TRUE)
m<-svm(DD~.,data =lagDD ,type="eps-regression"
,kernel="radial",scale=FALSE, cost=cost,gamma=gamma)
new <- predict(m, lagDD[,-1])
sum((new-lagDD[,1])^2)/length(new)
}
